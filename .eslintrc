{
  "env": {
    "browser": true,
    "commonjs": true,
    "embertest": true,
    "es6": true,
    "jquery": true,
    "node": true,
    "phantomjs": true,
    "qunit": true
  },
  "extends": "eslint:recommended",
  "globals": {
    "Promise": false,
    "Clipboard": false,
    "document": false,
    "katex": false,
    "MathQuill": false,
    "moment": false,
    "SockJS": false,
    "Stomp": false,
    "Switchery": false,
    "wait": false,
    "window": false,
    "wysihtml5": false,
    "wysihtml5ParserRules": false
  },
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "comma-dangle": 2,
    "curly": 2,
    "dot-notation": 1,
    "eqeqeq": [
      2,
      "smart"
    ],
    "indent": [
      0,
      2
    ],
    "linebreak-style": [
      2,
      "unix"
    ],
    "max-len": 0,
    "no-caller": 2,
    "no-duplicate-imports": 2,
    "no-empty-function": 2,
    "no-eval": 2,
    "no-extra-bind": 2,
    "no-multiple-empty-lines": 0,
    "no-new-func": 2,
    "no-new-wrappers": 2,
    "no-plusplus": [
      2,
      {
        "allowForLoopAfterthoughts": true
      }
    ],
    "no-trailing-spaces": 2,
    "no-unneeded-ternary": 2,
    "no-use-before-define": 1,
    "no-useless-call": 2,
    "no-useless-concat": 2,
    "no-useless-escape": 2,
    "prefer-const": 0,
    "prefer-template": 1,
    "quotes": [
      0,
      "single"
    ],
    "require-jsdoc": 1,
    "semi": [
      2,
      "always"
    ]
  }
}
