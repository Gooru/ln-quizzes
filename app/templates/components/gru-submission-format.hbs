{{#each submissionFormats as |submissionFormat|}}
    <div class="panel panel-default {{concat 'submission-format-' submissionFormat}} {{set-if-equal 'active' submissionFormat selectedType}}" {{action 'selectType' questionType}}>
        <div class="panel-body">
            <i class="format-icon"></i>
        </div>
        <div class="panel-footer">
          {{t (concat 'gru-submission-format.' submissionFormat) }}
        </div>
    </div>
{{/each}}
